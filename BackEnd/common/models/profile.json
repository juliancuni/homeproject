{
  "name": "Profile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Port": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "clients": {
      "type": "hasMany",
      "model": "Client",
      "foreignKey": "profileId",
      "through": "mapNewcamdClient"
    },
    "peers": {
      "type": "hasMany",
      "model": "Peer",
      "foreignKey": "camId"
    }
  },
  "acls": [],
  "methods": {}
}
